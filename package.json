{
  "name": "ai-rideshare-platform",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered rideshare platform with smart matching, dynamic pricing, route optimization, and predictive analytics",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "analyze": "npm run build && start dist/stats.html",
    "fix-mui-imports": "node scripts/fix-mui-imports.js",
    "generate-icons": "node scripts/generate-icons.js",
    "pwa:test": "npm run build && npm run preview"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.28.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@livekit/react-components": "^1.1.0",
    "@loadable/component": "^5.16.7",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/leaflet": "^1.9.21",
    "ai": "^6.0.48",
    "axios": "^1.7.9",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.18.2",
    "history": "^5.3.0",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "leaflet.heat": "^0.2.0",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.17.0",
    "next-pwa": "^5.6.0",
    "notistack": "^3.0.1",
    "openai": "^6.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.71.0",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-map-gl": "^7.1.9",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.3",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^2.15.4",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.8.3",
    "workbox-core": "^7.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-modules-commonjs": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@eslint/js": "^9.9.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^9.8.8",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "msw": "^2.12.10",
    "postcss": "^8.5.6",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-nesting": "^13.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-window": "^7.4.0"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1",
    "^@/hooks$": "<rootDir>/src/hooks/index.js",
    "^@/context/(.*)$": "<rootDir>/src/context/$1",
    "^@/styles/(.*)$": "<rootDir>/src/styles/$1",
    "^@/layouts/(.*)$": "<rootDir>/src/layouts/$1",
    "^@/pages/(.*)$": "<rootDir>/src/pages/$1",
    "\\.(css|less|scss|sass)$": "identity-obj-proxy",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
  },
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!axios|react-hot-toast|@emotion|@mui)/"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx",
    "ts",
    "tsx",
    "json",
    "node"
  ],
  "testMatch": [
    "**/__tests__/**/*.[jt]s?(x)",
    "**/?(*.)+(spec|test).[jt]s?(x)"
  ],
  "collectCoverage": false,
  "testEnvironmentOptions": {
    "url": "http://localhost:8000",
    "customExportConditions": [
      ""
    ]
  },
  "globals": {
    "import.meta.env": {
      "VITE_API_URL": "http://localhost:8000/api",
      "VITE_WS_URL": "ws://localhost:8000"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
